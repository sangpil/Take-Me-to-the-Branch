{
  "name": "take-me-to-the-branch",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Take Me to the Branch",
    "manifest": "src/manifest.json",
    "main": "take-me-to-the-branch.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
